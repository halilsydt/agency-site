# Environment Variables
# Copy this file to .env.local and fill in the values

# Formspree (Contact Form)
NEXT_PUBLIC_FORMSPREE_ENDPOINT=

# Cal.com (Booking)
# Format: username/event-type (e.g., marketplace-consultants/free-consultation)
NEXT_PUBLIC_CAL_LINK=

# Analytics (Plausible)
# Format: yourdomain.com (without https://)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=

# ConvertKit (Newsletter)
# Find these at: https://app.convertkit.com/account_settings/advanced_settings
NEXT_PUBLIC_CONVERTKIT_FORM_ID=
NEXT_PUBLIC_CONVERTKIT_API_KEY=

# Cookie Consent
# Set to "false" to disable cookie consent banner (e.g., non-EU audience)
NEXT_PUBLIC_COOKIE_CONSENT_ENABLED=true
